import re
line = input("seq:")
A = len(re.findall("A|a", line)) / len(line)
T = len(re.findall("T|t", line)) / len(line)
C = len(re.findall("C|c", line)) / len(line)
G = len(re.findall("G|g", line)) / len(line)
print ("A:",A)
print ("T:",T)
print ("C:",C)
print ("G:",G)
"""
   Example:
        input part:
            seq:ATCGGACC
        output part:
            A：0.25
            T：0.125
            C：0.375
            G：0.25   
"""
